    },
    "tray": {
        "spacing": 10
    },
    "clock": {
        "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
        "format-alt": "{:%Y-%m-%d}"
    },
    "cpu": {
    "format": "{usage}% <span size='x-large' rise='-1500'>ï‹›</span>",
    "tooltip": false
},
"memory": {
    "format": "{}% <span size='x-large' rise='-1500'>ïƒ‰</span>"
},
    "temperature": {
        "critical-threshold": 80,
        "format": "{temperatureC}Â°C <span size='x-large' rise='-1500'>{icon}</span>",
        "format-icons": ["ï«", "ï‹‰", "ï©"]
